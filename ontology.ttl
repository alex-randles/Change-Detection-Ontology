###  https://change-detection-ontology.adaptcentre.ie/#hasEvent
:hasEvent rdf:type owl:ObjectProperty ;
          owl:inverseOf :inEventLog ;
          rdfs:domain :ChangeLog ;
          rdfs:range <http://linkedevents.org/ontology/Event> ;
          rdfs:comment "An event within a certain event log." ;
          rdfs:label "hasEvent" .


###  https://change-detection-ontology.adaptcentre.ie/#hasOwner
:hasOwner rdf:type owl:ObjectProperty ;
          owl:inverseOf :ownedBy ;
          rdfs:domain :ChangeLog ;
          rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent> ;
          rdfs:comment "The owner of the event log." ;
          rdfs:label "hasOwner" .


###  https://change-detection-ontology.adaptcentre.ie/#inEventLog
:inEventLog rdf:type owl:ObjectProperty ;
            rdfs:domain <http://linkedevents.org/ontology/Event> ;
            rdfs:range :ChangeLog ;
            rdfs:comment "An event within an event log." ;
            rdfs:label "inEventLog" .


###  https://change-detection-ontology.adaptcentre.ie/#ownedBy
:ownedBy rdf:type owl:ObjectProperty ;
         rdfs:comment "The agent who owns the change log." ;
         rdfs:label "ownedBy" .


